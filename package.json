{
  "name": "ai-menstrual-wellness-assistant",
  "version": "1.0.0",
  "description": "AI Menstrual Wellness Assistant - Full Stack Application",
  "scripts": {
    "build": "cd backend && pip install -r requirements.txt",
    "deploy": "gunicorn --bind 0.0.0.0:${PORT:-8080} --workers 4 --timeout 0 app:app",
    "dev:backend": "cd backend && python app.py",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm install && npm run build",
    "start": "npm run deploy"
  },
  "dependencies": {
    "firebase": "^12.5.0",
    "lucide-react": "^0.552.0",
    "openai": "^6.8.1",
    "react-router-dom": "^6.30.1"
  }
}
